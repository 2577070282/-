.tabbar_box {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 98rpx;
    border-top: 1rpx solid #e1e1e6;
}

.tabbar_nav {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 22rpx;
    height: 100%;
    margin-right: 3rpx;
    width: 100%;
    position: relative;
}

.tabbar_icon {
    width: 50rpx;
    height: 50rpx;
}

.dot_tar {
    width: 12rpx;
    height: 12rpx;
    border-radius: 50%;
    background: rgb(210,26,18);
    position: absolute;
    top: 11%;
    left: 59%;
}