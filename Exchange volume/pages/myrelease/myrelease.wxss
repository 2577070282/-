.middle {
    position: relative;
}

.icn_01_one {
    width: 130rpx;
    height: 130rpx;
    border-radius: 26rpx;
    position: relative;
    margin-right: 30rpx;
}

.list_bottom {
    display: flex;
    padding: 30rpx;
    position: relative;
    align-items: center;
}

.line {
    position: absolute;
    width: 92.5%;
    height: 2rpx;
    background: rgb(226,225,230);
    left: 30rpx;
    bottom: 0;
}

.list_bottom_top {
    width: 62%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.list_bottom_name {
    font-size: 32rpx;
    font-weight: 700;
    position: relative;
    display: flex;
    margin-bottom: 13rpx;
    width: 100%;
    align-items: center;
}

.list_bottom_name_list {
    position: relative;
    width: 100%;
    display: flex;
}

.list_bottom_name_game {
    margin-right: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.img_sell_01 {
    width: 30rpx;
    height: 30rpx;
}

.list_bottom_type {
    font-size: 24rpx;
    color: rgb(102,102,102);
}

.list_bottom_info {
    font-size: 24rpx;
    color: rgb(153,153,153);
    display: flex;
    flex-wrap: wrap;
}

.list_bottom_right {
    font-size: 20rpx;
    text-align: center;
    display: flex;
    flex-direction: column;
}

.list_bottom_right_num {
    font-size: 32rpx;
    font-weight: 700;
    margin-bottom: 8rpx;
}

.list_bottom_right_day {
    background: rgb(251,237,236);
    color: rgb(210,26,18);
    letter-spacing: 2px;
    height: 40rpx;
    line-height: 40rpx;
    width: 140rpx;
    text-align: center;
    border-radius: 20rpx;
}

.button_bototm_center {
    position: absolute;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.btn_delete_black {
    width: 28rpx;
    height: 28rpx;
    padding: 8rpx 8rpx 0;
}

.state {
    font-size: 24rpx;
    color: rgb(153,153,153);
    font-weight: 400;
    text-align: right;
}

.list_bottom_right_state {
    width: 99rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    height: 118rpx;
}

.btn_delete_black_bg {
    width: 100%;
    display: flex;
    justify-content: space-between;
}

.otherlist_bottom_list {
    flex-direction: column;
    padding: 0 20rpx 40rpx;
    margin: 40rpx 30rpx;
}

.otherlist_bottom_list .line {
    left: 10rpx;
    width: 98%;
}

.otherlist_bottom_list_item {
    display: flex;
    justify-content: space-between;
    height: 60rpx;
    align-items: center;
    border-radius: 20rpx 20rpx 20rpx 0;
    background: rgb(245,245,250);
    font-size: 32rpx;
    font-weight: 700;
    width: 650rpx;
    padding: 0 20rpx;
    margin-bottom: 30rpx;
    position: relative;
}

.otherlist_bottom_list_time {
    font-size: 28rpx;
    color: rgb(153,153,153);
    font-weight: 400;
}

.connection {
    width: 650rpx;
    padding: 0 20rpx;
    font-size: 28rpx;
    color: rgb(42,42,42);
    display: flex;
}

.tel {
    margin: 25rpx 0;
}

.otherdec {
    color: rgb(153,153,153);
    font-size: 28rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-items: center;
    padding: 0 20rpx;
    width: 648rpx;
}

.otherdec_bg {
    width: 78%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.nickName {
    width: 78%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.other_btn_delete_black_bg {
    width: 99rpx;
}

.img_nothing {
    width: 220rpx;
    height: 152rpx;
}

.no_search {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    text-align: center;
}

.no_search view {
    color: rgb(153,153,153);
    font-size: 24rpx;
    margin: 24rpx auto;
}

.no_search button {
    background: rgb(210,26,18);
    color: #fff;
    height: 70rpx;
    line-height: 70rpx;
    width: 240rpx;
    border-radius: 35rpx;
    font-size: 28rpx;
}

.list_bottom_info_item {
    display: flex;
    margin-top: 13rpx;
}

.list_bottom_info_item view {
    padding: 9rpx 16rpx 8rpx 20rpx;
    background: rgb(239,239,244);
    border-radius: 18rpx;
    margin-right: 10rpx;
    color: rgb(42,42,42);
}

.img_WeChat_toast {
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.6);
    display: none;
    z-index: 4;
}

.img_WeChat_toast_white {
    width: 570rpx;
    height: 340rpx;
    background: #fff;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    border-radius: 20rpx;
    font-size: 32rpx;
    text-align: center;
    display: none;
    z-index: 5;
}

.img_WeChat_toast_white_bg {
    height: 100%;
    width: 100%;
    position: relative;
}

.img_WeChat_toast_white_top {
    position: absolute;
    left: 50%;
    top: 109rpx;
    transform: translate(-50%);
    width: 80%;
    color: rgb(42,42,42);
}

.img_WeChat_toast_white_bottom {
    display: flex;
    position: absolute;
    bottom: 0;
    width: 100%;
    color: rgb(153,153,153);
    height: 100rpx;
    line-height: 100rpx;
    border-top: 1rpx solid #eeedf2;
}

.img_WeChat_toast_white_bottom view {
    width: 50%;
    height: 100%;
}

.img_WeChat_toast_white_bottom view:nth-last-child(1) {
    color: rgb(210,26,18);
    border-left: 1rpx solid #eeedf2;
}

.toast {
    display: block;
}